#TABUADA COM while

print('TABUADA \n')
n=int(input('Digite o número que deseja obter a tabuada: '))
print('\n')
i=1
while i<=10:
    print('{}x{} = {}'.format(i,n,i*n))
    i+=1

#TABUADA COM for

n=int(input('Digite o número desejado: '))
x=int(input('Digite até que valor deseja a tabuada deste número: '))

print('\nTABUADA DE {}\n'.format(n))
for i in range(1,x+1,1):
    print('{} x {} = {}'.format(n,i,n*i))
